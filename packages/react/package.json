{
  "name": "@eo-locale/react",
  "version": "1.4.15",
  "description": "Easy way to localize your React application",
  "author": "Maksim Sharipov <m.v.sharipov@gmail.com>",
  "homepage": "https://eo-locale.netlify.app",
  "license": "ISC",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "type": "module",
  "exports": {
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup --config config/rollup.config.js",
    "postbuild": "copyup \"src/**/*.flow\" lib && size-limit",
    "prepare": "npm run test && npm run build",
    "test": "jest --detectOpenHandles --config config/jest.config.json --no-cache"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibitcy/eo-locale.git"
  },
  "bugs": {
    "url": "https://github.com/ibitcy/eo-locale/issues"
  },
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "react",
    "tiny",
    "i18n"
  ],
  "size-limit": [
    {
      "path": "lib/index.cjs",
      "limit": "950B",
      "webpack": false
    }
  ],
  "dependencies": {
    "@eo-locale/core": "^1.4.15"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/enzyme": "^3.10.6",
    "@types/jest": "^26.0.13",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "copyfiles": "^2.3.0",
    "enzyme": "^3.11.0",
    "flow-bin": "^0.130.0",
    "jest": "^26.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.26.11",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "size-limit": "^4.6.0",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.2",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "gitHead": "85f16888e69bd8962e3e20ad92065a6ef5afcb22"
}
